{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "nx exec -- nx exec -- next build",
    "start": "next start",
    "lint": "nx exec -- nx exec -- next lint",
    "lint:fix": "nx exec -- nx exec -- next lint --fix",
    "preview": "next build && next start",
    "typecheck": "nx exec -- nx exec -- tsc --noEmit",
    "format:write": "nx exec -- nx exec -- prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "nx exec -- nx exec -- prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "format:all": "nx exec -- nx exec -- prettier --config .prettierrc --ignore-path .prettierignore --write ."
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.291.0",
    "million": "^2.6.4",
    "next": "latest",
    "next-themes": "^0.2.1",
    "react": "latest",
    "react-dom": "latest",
    "sharp": "^0.32.6",
    "tailwind-merge": "^2.1.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@nx/next": "17.0.3",
    "@types/node": "^20.8.9",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/parser": "^6.9.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.52.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "nx": "17.0.3"
  },
  "engines": {
    "node": ">=18"
  },
  "nx": {
    "targets": {}
  }
}

